   <div class="panel panel-default">
    <div class="panel-heading">
      <div class="row panel-title">
        <div class="col-md-3 date">
          {{dateText}}
          <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapse{{_id}}">
            <i class="glyphicon glyphicon-circle-arrow-down"></i>
          </a>
        </div>
        <div class="col-md-3 event-title">
          <a class="edit">{{name}}</a>
        </div>
        <div class="col-md-3 business-name">
          {{businessName}}
        </div>
        <div class="col-md-3">
          <a class="deleteButton"><i class="glyphicon glyphicon-trash"></i></a>
        </div>                 
      </div>
    </div>
    <div id="collapse{{_id}}" class="panel-collapse collapse">
      <div class="panel-body">
        {{#each occurrences}}
          <div class="row">
            <div class="col-md-3 col-md-offset-1 recurring-date">{{dateFormat this format="MM/DD/YYYY"}}</div>
          </div>
        {{/each}}
      </div>
    </div>
  </div>   