
<div class="row event-list-controls">
  <div span class="col-md-3">
    {{#if allowCreate}}
      <a class="btn btn-success createButton">Create New Event <i class="glyphicon glyphicon-cloud-upload"></i></a>
    {{else}}
      <a class="btn btn-success createButton" disabled="disabled" title="You need a business first!">Create New Event <i class="glyphicon glyphicon-cloud-upload"></i></a>
    {{/if}}
  </div>
  <div span class="col-md-6 alert-container"></div>
  <div span class="col-md-3">
    <div class="btn-group pull-right list-options">
      <a href="/myEvents/past" class="btn btn-default">Past Events</a>
      <a href="/myEvents" class="btn btn-info">Upcoming Events</a>
    </div>
  </div>
</div>
<div class="newItem"></div>
<div class="panel-group clear" id="accordion"></div>
