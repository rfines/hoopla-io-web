 <div class="row">
  <div class="col-sm-7 col-lg-4 event-pages"></div>
  </div>
<div class="row">
  <img src={{coverPhoto}} alt="facebook cover image" />
  <h4>{{name}}</h4>
  <div class="event-times"><span>{{dayOfWeek}}</span><span>{{startDate}}</span><span>Sunny and 75</span></div>
  <div class="event-description"><span>{{description}}</span></div>
  <div class="event-map" style="height:100px;width:500px;"></div>
</div><br />
<div class="row" style="margin-top:20%;">
  <div class="media">
    <a class="pull-left" href="#">
      <img class="media-object" src={{profileImgUrl}} alt="facebook profile picture">
    </a>
    <div class="media-body">
      <h4 class="media-heading">{{profileName}}</h4>
      <div class="preview-message"><div class="event-times"><span>{{dayOfWeek}} at {{startDate}}</span></div>
      <div class="event-address"><span>{{eventAddress}}</span></div>
    </div>
  </div>

</div>
<div class="actions" style="margin-top:5%;">
  <button type="button" class="btn btn-success createFbEventBtn">Continue</button>
  <button type="button" class="btn btn-cancel cancelBtn">Return to Events</button>
</div>
